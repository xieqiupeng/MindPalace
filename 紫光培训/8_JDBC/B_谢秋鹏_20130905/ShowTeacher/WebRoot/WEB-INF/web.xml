<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletInsertTeacher</servlet-name>
    <servlet-class>com.PackageServlet.ServletInsertTeacher</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletTeacherList</servlet-name>
    <servlet-class>com.PackageServlet.ServletTeacherList</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletTeacherInfo</servlet-name>
    <servlet-class>com.PackageServlet.ServletTeacherInfo</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletUpdateTeacher</servlet-name>
    <servlet-class>com.PackageServlet.ServletUpdateTeacher</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletToUpdateTeacher</servlet-name>
    <servlet-class>com.PackageServlet.ServletToUpdateTeacher</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletDisableTeacher</servlet-name>
    <servlet-class>com.PackageServlet.ServletDisableTeacher</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>ServletInsertTeacher</servlet-name>
    <url-pattern>/servlet/ServletInsertTeacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletTeacherList</servlet-name>
    <url-pattern>/servlet/ServletTeacherList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletTeacherInfo</servlet-name>
    <url-pattern>/servlet/ServletTeacherInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletUpdateTeacher</servlet-name>
    <url-pattern>/servlet/ServletUpdateTeacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletToUpdateTeacher</servlet-name>
    <url-pattern>/servlet/ServletToUpdateTeacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletDisableTeacher</servlet-name>
    <url-pattern>/servlet/ServletDisableTeacher</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
